# Server
SERVER_PORT=8080

# Database
DATABASE_URL=postgres://engram:engram@localhost:5432/engram?sslmode=disable

# Migrations
MIGRATIONS_PATH=migrations

# LLM Provider (openai, anthropic, gemini, cerebras, mock)
LLM_PROVIDER=openai
EMBEDDING_PROVIDER=openai

# API Keys (store in .env.secret or .env.development.local.secret)
# OPENAI_API_KEY=sk-...
# ANTHROPIC_API_KEY=sk-ant-...
# GEMINI_API_KEY=...
# CEREBRAS_API_KEY=...

# Rate Limiting
RATE_LIMIT_RPS=100
RATE_LIMIT_BURST=20

# Logging
LOG_LEVEL=info
